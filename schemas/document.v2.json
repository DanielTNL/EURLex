    "canonical_url": { "type": ["string","null"], "format": "uri" },
    "fetch_time": { "type": "string", "format": "date-time" },
    "language": { "type": ["string","null"] },
    "title": { "type": "string" },
    "published_date": { "type": "string", "format": "date-time" },
    "updated_date": { "type": ["string","null"], "format": "date-time" },
    "doc_type": { "type": "string" },
    "programme": { "type": "array", "items": { "type": "string" } },
    "finance_instrument": { "type": "array", "items": { "type": "string" } },
    "stage": { "type": ["string","null"] },
    "actors": { "type": "array", "items": { "type": "object" } },
    "tech_area": { "type": "array", "items": { "type": "string" } },
    "monetary_values": { "type": "array", "items": { "type": "object" } },
    "summary_150w": { "type": "string" },
    "key_points": { "type": "array", "items": { "type": "string" } },
    "implications": { "type": "object" },
    "links": { "type": "object" },
    "celex_id": { "type": ["string","null"] },
    "call_id": { "type": ["string","null"] },
    "award_id": { "type": ["string","null"] },
    "tags": { "type": "array", "items": { "type": "string" } },
    "dedupe_signature": { "type": ["string","null"] },
    "embeddings": { "type": ["object","null"] },
    "extraction_notes": { "type": ["string","null"] }
  },
  "additionalProperties": true
}
